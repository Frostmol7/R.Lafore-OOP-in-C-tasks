#include <iostream>
using namespace std;
int k=0;

void fun()
{
    static int i = 0;
    cout << "works "<< i << " times"<<endl;
    i++;
}

void unfun()
{
    cout << "works "<< k << " times"<<endl;
    k++;
}

int main()
{
    for (int j = 0; j<15 ; j++)
        fun();

cout << endl;

    for (int j = 0; j<15 ; j++)
        unfun();

    return 0;
}
